{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-msedge",
      "request": "launch",
      "name": "Launch Edge with current file",
      "runtimeExecutable": "C:/Program Files (x86)/Microsoft/Edge/Application/msedge.exe",
      "url": "file:///${file}",
      "args": ["--remote-debugging-port=9333"],
      "webRoot": "${workspaceFolder}"
    }
  ]
}
